    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-promise-router": "^4.1.1",
    "ts-node": "^10.3.0",
    "typescript": "^4.5.3"
  },
  "devDependencies": {
    "nodemon": "^2.0.15"
  },
  "git repository": "https://github.com/open-template-hub/mail-server-template",
  "repository": {
